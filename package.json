{
  "name": "uncreate",
  "version": "0.7.1",
  "description": "Minimal, type-safe REST client using JS proxies",
  "keywords": [
    "ohmyfetch",
    "fetch",
    "api"
  ],
  "homepage": "https://github.com/johannschopplich/uncreate#readme",
  "bugs": {
    "url": "https://github.com/johannschopplich/uncreate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/uncreate.git"
  },
  "license": "MIT",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "test": "vitest",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "release": "bumpp package.json --commit --push --tag"
  },
  "dependencies": {
    "defu": "^6.0.0",
    "ohmyfetch": "^0.4.15",
    "ufo": "^0.8.3"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "h3": "^0.7.4",
    "listhen": "^0.2.10",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "vitest": "^0.9.3"
  }
}
