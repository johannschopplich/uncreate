{
  "name": "uncreate",
  "type": "module",
  "version": "0.8.0",
  "packageManager": "pnpm@7.1.7",
  "description": "Minimal, type-safe REST client using JS proxies",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/uncreate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/uncreate.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/uncreate/issues"
  },
  "keywords": [
    "ohmyfetch",
    "fetch",
    "api"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp package.json --commit --push --tag"
  },
  "dependencies": {
    "defu": "^6.0.0",
    "ohmyfetch": "^0.4.18",
    "ufo": "^0.8.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "h3": "^0.7.8",
    "listhen": "^0.2.11",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "vitest": "^0.13.1"
  }
}
